device crypt		# OpenSSL cryptographic library
options GEOM_ELI	# control utility for the cryptographic GEOM class
device aesni		# driver for the AES accelerator on Intel CPUs
options P1003_1B_SEMAPHORES


device run		# Ralink Technology USB IEEE 802.11a/g/n wireless network device
device runfw		# Firmware Module for Ralink driver

device acpi_ibm		# ACPI extras driver for IBM laptops
device acpi_video	# ACPI Video Extensions driver
device coretemp		# device	driver for Intel Core on-die digital thermal sensor	

device iicbb		# I2C generic bit-banging driver
device iic		# I2C generic I/O device driver
device iicbus		# I2C bus system

options  KRPC

